<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="CodeSamplesFrame">
		<size>800,480</size>
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<pos>288,120</pos>
			<object class="wxButton" name="ID_BUTTON1" variable="Button1" member="yes">
				<label>Geometric Lines</label>
				<pos>32,40</pos>
				<size>168,34</size>
				<handler function="OnButton1Click" entry="EVT_BUTTON" />
			</object>
			<object class="wxPanel" name="ID_PANEL2" variable="drawPanel" member="yes">
				<pos>240,24</pos>
				<size>536,432</size>
				<handler function="OndrawPanelPaint" entry="EVT_PAINT" />
			</object>
			<object class="wxButton" name="ID_BUTTON2" variable="Button2" member="yes">
				<label>Colored Lines</label>
				<pos>32,96</pos>
				<size>168,34</size>
				<handler function="OnButton2Click" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="ID_BUTTON3" variable="Button3" member="yes">
				<label>Brush Styles</label>
				<pos>32,144</pos>
				<size>168,34</size>
				<handler function="OnButton3Click" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="ID_BUTTON4" variable="Button4" member="yes">
				<label>Fonts</label>
				<pos>32,192</pos>
				<size>168,34</size>
				<handler function="OnButton4Click" entry="EVT_BUTTON" />
			</object>
			<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="StaticBitmap1" member="yes">
